{
  "name": "pinger",
  "private": true,
  "version": "0.3.1",
  "description": "",
  "scripts": {
    "coverage": "jest --coverage",
    "docs-dev": "ocedocs -w",
    "lint": "eslint web",
    "preversion": "eslint web && ocedocs && git add docs",
    "prettier": "prettier --write ./**/*.{js,json,css,md}",
    "start": "NODE_ENV=production node web",
    "test": "jest",
    "dev": "NODE_ENV=development nodemon web --watch web"
  },
  "author": "Rob Anderson (https://r0b.io)",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,less,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "api-formatter": "^1.0.0",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ocelot-docs": "^0.4.0"
  }
}
